<?xml version="1.0" encoding="UTF-8"?>
<_config:DataloadBusinessObjectConfiguration
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.ibm.com/xmlns/prod/commerce/foundation/config ../../../../xml/config/xsd/wc-dataload-businessobject.xsd"
	xmlns:_config="http://www.ibm.com/xmlns/prod/commerce/foundation/config">
	
	<_config:DataLoader className="com.ibm.commerce.foundation.dataload.BusinessObjectLoader" >   
		<_config:DataReader className="com.ibm.commerce.foundation.dataload.datareader.CSVReader" firstLineIsHeader="true" >
			<_config:Data>
				<_config:column number="1" name="catentry_id" />
				<_config:column number="2" name="days_to_display_after_oos" />
				<_config:column number="3" name="enable_ntfctn_ind" />
				<_config:column number="4" name="enable_survey_flag" />
				<_config:column number="5" name="erp_prod_type" />
				<_config:column number="6" name="inc_in_srch_rslts" />
				<_config:column number="7" name="max_order_line_qty" />
				<_config:column number="8" name="pre_order_flag" />
				<_config:column number="9" name="allow_prod_review" />
				<_config:column number="10" name="prod_rating_bv" />
				<_config:column number="11" name="view_prod_detail_req" />
				<_config:column number="12" name="reservable" />
				<_config:column number="13" name="returnable" />
				<_config:column number="14" name="tax_code" />
				<_config:column number="15" name="canonical_cat" />
				<_config:column number="16" name="ship_alone_flag" />
				<_config:column number="17" name="display_if_not_avail" />
				<_config:column number="18" name="cust_seg_exclusive" />
				<_config:column number="19" name="financial_brand" />
				<_config:column number="20" name="ltd_qty_msg_override" />
				<_config:column number="21" name="new_override_flag" />
				<_config:column number="22" name="popularity_rating" />
				<_config:column number="23" name="product_status_code" />
				<_config:column number="24" name="sale_override_flag" />
				<_config:column number="25" name="find_a_retailer_flag" />
				<_config:column number="26" name="alt_review_sku" />
				<_config:column number="27" name="cogs" />
				<_config:column number="28" name="suppress_order_line" />
				<_config:column number="29" name="ship_as_complete" />
				<_config:column number="30" name="inc_std_shipping" />
				<_config:column number="31" name="optcounter" />
				<_config:column number="32" name="releasedate" />
			</_config:Data>
		</_config:DataReader>
		
		<_config:BusinessObjectBuilder className="com.ibm.commerce.foundation.dataload.businessobjectbuilder.TableObjectBuilder" >
			<!-- 
						CATENTRY_ID               NOT NULL NUMBER        
						DAYS_TO_DISPLAY_AFTER_OOS          NUMBER        
						ENABLE_NTFCTN_IND                  CHAR(1)       
						ENABLE_SURVEY_FLAG                 CHAR(1)       
						ERP_PROD_TYPE                      VARCHAR2(64)  
						INC_IN_SRCH_RSLTS                  CHAR(1)       
						MAX_ORDER_LINE_QTY                 NUMBER        
						PRE_ORDER_FLAG                     CHAR(1)       
						ALLOW_PROD_REVIEW                  CHAR(1)       
						PROD_RATING_BV                     NUMBER        
						VIEW_PROD_DETAIL_REQ               CHAR(1)       
						RESERVABLE                         NUMBER        
						RETURNABLE                         NUMBER        
						TAX_CODE                           VARCHAR2(128) 
						CANONICAL_CAT                      VARCHAR2(128) 
						SHIP_ALONE_FLAG                    CHAR(1)       
						DISPLAY_IF_NOT_AVAIL               CHAR(1)       
						CUST_SEG_EXCLUSIVE                 VARCHAR2(256) 
						FINANCIAL_BRAND                    VARCHAR2(128) 
						LTD_QTY_MSG_OVERRIDE               CHAR(1)       
						NEW_OVERRIDE_FLAG                  CHAR(1)       
						POPULARITY_RATING                  NUMBER        
						PRODUCT_STATUS_CODE                VARCHAR2(128) 
						SALE_OVERRIDE_FLAG                 CHAR(1)       
						FIND_A_RETAILER_FLAG               CHAR(1)       
						ALT_REVIEW_SKU                     VARCHAR2(128) 
						COGS                               NUMBER        
						SUPPRESS_ORDER_LINE                CHAR(1)       
						SHIP_AS_COMPLETE                   CHAR(1)       
						INC_STD_SHIPPING                   CHAR(1)       
						OPTCOUNTER                         NUMBER        
						RELEASEDATE                        DATE 			
			 -->
			<_config:Table name="XCATENTRY">
				<_config:Column name="CATENTRY_ID" value="catentry_id" />
				<_config:Column name="DAYS_TO_DISPLAY_AFTER_OOS" value="days_to_display_after_oos" />
				<_config:Column name="ENABLE_NTFCTN_IND" value="enable_ntfctn_ind" />
				<_config:Column name="ENABLE_SURVEY_FLAG" value="enable_survey_flag" />
				<_config:Column name="ERP_PROD_TYPE" value="erp_prod_type" />
				<_config:Column name="INC_IN_SRCH_RSLTS" value="inc_in_srch_rslts" />
				<_config:Column name="MAX_ORDER_LINE_QTY" value="max_order_line_qty" />
				<_config:Column name="PRE_ORDER_FLAG" value="pre_order_flag" />
				<_config:Column name="ALLOW_PROD_REVIEW" value="allow_prod_review" />
				<_config:Column name="PROD_RATING_BV" value="prod_rating_bv" />
				<_config:Column name="VIEW_PROD_DETAIL_REQ" value="view_prod_detail_req" />
				<_config:Column name="RESERVABLE" value="reservable" />
				<_config:Column name="RETURNABLE" value="returnable" />
				<_config:Column name="TAX_CODE" value="tax_code" />
				<_config:Column name="CANONICAL_CAT" value="canonical_cat" />
				<_config:Column name="SHIP_ALONE_FLAG" value="ship_alone_flag" />
				<_config:Column name="DISPLAY_IF_NOT_AVAIL" value="display_if_not_avail" />
				<_config:Column name="CUST_SEG_EXCLUSIVE" value="cust_seg_exclusive" />
				<_config:Column name="FINANCIAL_BRAND" value="financial_brand" />
				<_config:Column name="LTD_QTY_MSG_OVERRIDE" value="ltd_qty_msg_override" />
				<_config:Column name="NEW_OVERRIDE_FLAG" value="new_override_flag" />
				<_config:Column name="POPULARITY_RATING" value="popularity_rating" />
				<_config:Column name="PRODUCT_STATUS_CODE" value="product_status_code" />
				<_config:Column name="SALE_OVERRIDE_FLAG" value="sale_override_flag" />
				<_config:Column name="FIND_A_RETAILER_FLAG" value="find_a_retailer_flag" />
				<_config:Column name="ALT_REVIEW_SKU" value="alt_review_sku" />
				<_config:Column name="COGS" value="cogs" />
				<_config:Column name="SUPPRESS_ORDER_LINE" value="suppress_order_line" />
				<_config:Column name="SHIP_AS_COMPLETE" value="ship_as_complete" />
				<_config:Column name="INC_STD_SHIPPING" value="inc_std_shipping" />
				<_config:Column name="OPTCOUNTER" value="optcounter" />
				<_config:Column name="RELEASEDATE" value="releasedate" />				
			</_config:Table>
			
			<_config:BusinessObjectMediator className="com.ibm.commerce.foundation.dataload.businessobjectmediator.TableObjectMediator" />
			
		</_config:BusinessObjectBuilder>
		
	</_config:DataLoader>
</_config:DataloadBusinessObjectConfiguration>