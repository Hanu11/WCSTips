        <_config:mapping xpath="CatalogEntryAttributes/Attributes[8]/StringValue/value" value="EndOfServiceDate"  />   
        
        <_config:mapping xpath="CatalogEntryAttributes/Attributes[9]/Name" value="discontinueDate" valueFrom="Fixed"/>
